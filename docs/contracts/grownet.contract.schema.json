        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "type"
      ],
      "type": "object"
    },
    "typeDef": {
      "additionalProperties": true,
      "properties": {
        "fields": {
          "additionalProperties": {
            "type": "string"
