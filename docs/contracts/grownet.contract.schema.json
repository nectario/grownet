        "type": "string"
      },
      "type": "array"
    },
    "diffFromV4": {
      "additionalProperties": true,
      "type": [
        "object",
        "array"
      ]
    },
    "errorHandlingAndSafety": {
      "additionalProperties": true,
      "type": [
        "object",
        "array"
      ]
    },
    "evaluation": {
      "additionalProperties": true,
