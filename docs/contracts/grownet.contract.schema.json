        },
        "params": {
          "items": {
            "$ref": "#/$defs/paramDef"
          },
          "type": "array"
        },
        "returns": {
          "type": "string"
        },
        "signature": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "params",
        "returns",
        "signature"
      ],
      "type": "object"
    },
    "paramDef": {
