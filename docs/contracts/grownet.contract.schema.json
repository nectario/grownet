          },
          "type": "array"
        },
        "notes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "sameAs": {
          "type": "string"
        }
      },
      "required": [
        "fields",
        "methods"
      ],
      "type": "object"
    }
