  "additionalProperties": false,
  "properties": {
    "addendum": {
      "additionalProperties": true,
      "type": [
        "object",
        "array"
      ]
    },
    "behavior": {
      "additionalProperties": true,
      "type": "object"
    },
    "complianceChecklist": {
      "additionalProperties": true,
      "type": [
        "object",
        "array"
      ]
    },
    "coreInvariants": {
